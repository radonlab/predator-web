{
  "name": "predator-web",
  "version": "1.0.0",
  "description": "Web app of predator platform",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/radonlab/predator-web.git"
  },
  "author": "skylerlee <skyler.ac.lee@gmail.com>",
  "license": "MIT",
  "browserslist": "last 2 versions, ie 9",
  "scripts": {
    "clean": "rimraf ./dist",
    "serve": "webpack-dev-server --config build/webpack.dev.config.js",
    "start": "webpack-dashboard -- npm run dev",
    "build": "webpack --config build/webpack.prod.config.js",
    "lint": "eslint ./src && flow check",
    "rebuild": "run-s clean lint build",
    "profile": "cross-env NODE_ENV=analysis webpack --config build/webpack.prod.config.js --profile --json > stats.json",
    "analyse": "webpack-bundle-analyzer stats.json",
    "reanalyse": "run-s profile analyse"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.3.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.59.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dashboard": "^1.0.2",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "normalize.css": "^7.0.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "wavebell": "^0.1.3"
  }
}
