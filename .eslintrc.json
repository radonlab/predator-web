{
  "extends": [
    "standard",
    "plugin:react/recommended",
    "plugin:flowtype/recommended"
  ],
  "env": {
    "browser": true
  },
  "plugins": [
    "react",
    "flowtype"
  ],
  "rules": {
    "no-var": "error",
    "no-empty": "error",
    "no-console": "error",
    "object-curly-spacing": "error"
  }
}
